FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    top
    {
        type            zeroGradient;
    }
    leftair
    {
        type            zeroGradient;
    }
    leftwall
    {
        type            zeroGradient;
    }
    rightair
    {
        type            zeroGradient;
    }
    rightwall
    {
        type            zeroGradient;
    }
    bottomWall
    {
        type            zeroGradient;
    }
    backwall1
    {
        type            zeroGradient;
    }
    backwall2
    {
        type            zeroGradient;
    }
    frontwall1
    {
        type            zeroGradient;
    }
    frontwall2
    {
        type            zeroGradient;
    }
}


